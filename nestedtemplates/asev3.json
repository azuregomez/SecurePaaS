{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {    
        "aseVnetName": {
            "type": "string"
        },
        "aseSubnetName": {
            "type": "string"
        },
         "aseName" : {
            "type": "string"    
         }
    },   
    "resources": [   
         {
            "type": "Microsoft.Web/hostingEnvironments",
            "apiVersion": "2018-02-01",
            "name": "[parameters('aseName')]",
            "location": "[resourceGroup().location]",            
            "kind": "ASEV3",
            "properties": {
                "name": "[parameters('aseName')]",
                "location": "[resourceGroup().location]",
                "vnetName": "[parameters('aseVnetName')]",
                "vnetResourceGroupName": "[resourceGroup().name]",
                "vnetSubnetName": "[parameters('aseSubnetName')]",
                "virtualNetwork": {
                    "id": "[resourceId('Microsoft.Network/virtualNetworks', parameters('aseVnetName'))]",
                    "subnet": "[parameters('aseSubnetName')]"
                },
                "internalLoadBalancingMode": "Web, Publishing",
                "multiSize": "Standard_D2d_v4",
                "ipsslAddressCount": 0,
                "dnsSuffix": "[concat(parameters('aseName'), '.appserviceenvironment.net')]",
                "frontEndScaleFactor": 15,
                "suspended": false                
            }        
        }
     ]
}